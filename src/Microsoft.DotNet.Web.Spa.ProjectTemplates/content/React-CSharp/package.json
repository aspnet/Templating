{
  "name": "ReactSpa",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "dotnet run",
    "build:vendor": "node node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js",
    "build:app": "node node_modules/webpack/bin/webpack.js",
    "build": "npm run build:vendor && npm run build:app",
    "build:vendor:rel": "npm run build:vendor -- --env.prod",
    "build:app:rel": "npm run build:app -- --env.prod",
    "build:rel": "npm run build:vendor:rel && npm run build:app:rel"
  },
  "devDependencies": {
    "@types/history": "4.6.0",
    "@types/react": "15.0.35",
    "@types/react-dom": "15.5.1",
    "@types/react-hot-loader": "3.0.3",
    "@types/react-router": "4.0.12",
    "@types/react-router-dom": "4.0.5",
    "@types/webpack-env": "1.13.0",
    "aspnet-webpack": "^2.0.1",
    "aspnet-webpack-react": "^3.0.0",
    "awesome-typescript-loader": "3.2.1",
    "bootstrap": "3.3.7",
    "css-loader": "0.28.4",
    "event-source-polyfill": "0.0.9",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.2.1",
    "json-loader": "0.5.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router-dom": "4.1.1",
    "style-loader": "0.18.2",
    "typescript": "2.4.1",
    "url-loader": "0.5.9",
    "webpack": "2.5.1",
    "webpack-hot-middleware": "2.18.2"
  }
